{
  "name": "Qartde",
  "description": "Rahmani is the best WhatsApp bot",
  "keywords": ["bot", "node", "baileys", "whatsapp"],
  "logo": "https://files.catbox.moe/aktbgo.jpg",
  "repository": "https://github.com/Qartde/RAHMAN-SMD",
  "success_url": "/",
  "stack": "container",
  "env": {
    "PREFIX": {
      "description": "Enter a symbol for your bot's prefix, e.g. . , ! ? Enter only one symbol",
      "value": ".",
      "required": true
    },
    "AUTO_READ_MESSAGES": {
      "description": "Mark messages as read |yes| or |no|",
      "value": "no",
      "required": true
    },
    "AUTO_READ_STATUS": {
      "description": "Auto-see status updates |yes| or |no|",
      "value": "yes",
      "required": true
    },
    "AUTO_DOWNLOAD_STATUS": {
      "description": "Auto-download status updates |yes| or |no|",
      "value": "no",
      "required": true
    },
    "PM_PERMIT": {
      "description": "Allow private use? |yes| or |no|",
      "value": "no",
      "required": true
    },
    "BOT_NAME": {
      "description": "Enter your bot's name",
      "value": "RAHMANI-SMD",
      "required": true
    },
    "BOT_MENU_LINKS": {
      "description": "Bot menu image link",
      "value": "https://files.catbox.moe/aktbgo.jpg",
      "required": false
    },
    "PUBLIC_MODE": {
      "description": "Bot available to everyone? |yes| or |no|",
      "value": "yes",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Enter your Heroku API key",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Enter the name of your Heroku app",
      "required": true
    },
    "SESSION_ID": {
      "description": "Enter your bot's session ID",
      "value": "",
      "required": true
    },
    "OWNER_NAME": {
      "desc": "Enter your name",
      "required": true,
      "value": "Qartde"
    },
    "OWNER_NUMBER": {
      "desc": "Enter your phone number (without the + sign, e.g. 255xxxxxx)",
      "required": true,
      "value": "255693629079"
    },
    "WARN_COUNT": {
      "desc": "Number of warnings before action",
      "required": true,
      "value": "2"
    },
    "CHAT_BOT": {
      "desc": "Enable chatbot? |yes| or |no|",
      "required": true,
      "value": "no"
    },
    "STARTING_BOT_MESSAGE": {
      "description": "Send startup message? |yes| or |no|",
      "required": true,
      "value": "no"
    },
    "PRESENCE": {
      "description": "Bot presence: |online|, |typing|, |recording| or leave blank",
      "value": "",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-apt"
    },
    {
      "url": "heroku/nodejs"
    }
  ],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ]
}
